{
  "prettier.singleQuote": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.eol": "\n",
  "typescript.preferences.importModuleSpecifier": "relative",
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "vscode_custom_css.imports": ["file:///C:/Users/Dell/.vscode/custom.css"],
  "workbench.startupEditor": "none",
  "explorer.compactFolders": false,
  "npm.keybindingsChangedWarningShown": true,
  "editor.cursorWidth": 25,
  "http.proxyAuthorization": null,
  "github.gitProtocol": "ssh",
  "git.showPushSuccessNotification": true,
  "editor.minimap.autohide": true,
  "editor.stickyScroll.enabled": true,
  "editor.cursorBlinking": "smooth",
  "omnisharp.useModernNet": false,
  "window.zoomLevel": 0.6,
  "material-icon-theme.activeIconPack": "nest",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.fontSize": 19,
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.settings.applyToAllProfiles": [],
  "terminal.integrated.fontSize": 17,
  "liveServer.settings.donotShowInfoMsg": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.minimap.renderCharacters": true,
  "workbench.colorTheme": "GitHub Dark",
  "editor.fontFamily": "JetBrains Mono, Fira Code, Cascadia Code, Consolas, 'Courier New', monospace",
  "editor.lineHeight": 24,
  "editor.fontLigatures": true,
  "editor.matchBrackets": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.renderLineHighlight": "all",
  "editor.semanticHighlighting.enabled": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.scale": 2,
  "problems.decorations.enabled": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.renderWhitespace": "boundary",
  "workbench.tree.indent": 12,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.list.horizontalScrolling": true,
  "terminal.integrated.cursorBlinking": true,
  "editor.scrollBeyondLastLine": false,
  "window.autoDetectColorScheme": true,
  "editor.tokenColorCustomizations": {
    "[GitHub Dark]": {
      "functions": "#f78c6c",
      "variables": "#749df6",
      // "variables": "#e3e5e9",
      "strings": "#c3e88d",
      "comments": "#546e7a",
      "keywords": "#ff79c6"
    },
    "[GitHub Light]": {
      "functions": "#f78c6c",
      "variables": "#749df6",
      // "variables": "#e3e5e9",
      "strings": "#c3e88d",
      "comments": "#546e7a",
      "keywords": "#ff79c6"
    }
  },
  "workbench.colorCustomizations": {
    "[GitHub Dark]": {
      "editor.foreground": "#ffcc00",
      "editor.background": "#1a1b2324",
      "editorCursor.foreground": "#dfdfdf",
      "editor.lineHighlightBackground": "#424242",
      "editor.selectionBackground": "#003366",
      "editorBracketMatch.border": "#ef840b"
    }
  },
  "workbench.preferredLightColorTheme": "GitHub Dark",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "editor.fontWeight": "300",
  "errorLens.errorTextColor": "#ff5555",
  "errorLens.errorBackground": "#ff55551a",
  "errorLens.errorForeground": "#ff5555",
  "errorLens.errorMessageBackground": "#ff555510",
  "errorLens.errorBackgroundLight": "#ff55551a",
  "errorLens.errorForegroundLight": "#ff5555",
  "errorLens.errorBorderWidth": "2px",
  "errorLens.errorBorderColor": "#ff5555",
  "errorLens.warningTextColor": "#ffb86c",
  "errorLens.warningBackground": "#ffb86c1a",
  "errorLens.warningForeground": "#ffb86c",
  "errorLens.warningMessageBackground": "#ffb86c10",
  "errorLens.fontWeight": "bold",
  "errorLens.fontStyleItalic": true,
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info"],
  "errorLens.exclude": [],
  "git.autofetch": true,
  "workbench.productIconTheme": "bongocat",
  "git.enableSmartCommit": true
